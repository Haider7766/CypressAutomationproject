{
  "name": "cypressautomation",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
"scripts": {
  "test": "cypress run --reporter mochawesome --reporter-options reportDir=cypress/reports/json,overwrite=true,html=false,json=true",
  "merge-report": "mochawesome-merge cypress/reports/json/*.json > cypress/reports/report.json",
  "generate-report": "marge cypress/reports/report.json -f final-report -o cypress/reports/html",
  "test:report": "npm run test && npm run merge-report && npm run generate-report"
},
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^14.5.3",
    "cypress-mochawesome-reporter": "^4.0.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.2.0"
  }
}
